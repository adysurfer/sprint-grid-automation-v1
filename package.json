{
  "name": "sprint-grid-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prettier": "prettier --write \"src/**/*{.ts,.js,.json}\"",
    "lint": "eslint --fix \"src/**/*.{ts,js}\"",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "pretty": "cucumber-js --tags=@e2c e2e -f json:e2e/output/results.json -f ./node_modules/cucumber-pretty",
    "tests": "cucumber-js --tags=@e2c e2e -f json:e2e/output/results.json",
    "report:html": "node e2e/config/report.js",
    "test": "mocha ./tests/examples.test.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.2.4",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "chai": "^4.3.7",
    "cucumber": "^6.0.7",
    "cucumber-html-reporter": "^5.5.0",
    "cucumber-pretty": "^6.0.1",
    "mocha": "^10.2.0",
    "moment": "^2.26.0",
